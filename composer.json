{
    "name": "letsgolearn/clever-php-sdk",
    "description": "Clever API/SSO PHP library",
    "authors": [{
        "name": "Joshua Lawrence",
        "email": "lawrence.joshua@gmail.com"
    }],
    "homepage": "https://clever.com",
    "license": "MIT",
    "keywords": ["clever", "composer", "clever.com"],
    "require": {
        "php"    : ">=8.1",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0"
    },
    "autoload": {
        "psr-4": {
            "LGL\\Clever\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
